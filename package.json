    "semver": ">=6.3.1",
    "send": ">=0.19.0",
    "path-to-regexp": ">=0.1.10",
    "serve-static": ">=1.15.0",
    "body-parser": ">=1.20.2",
    "axios/follow-redirects": ">=1.15.4",
    "ip": "https://registry.npmjs.org/neoip/-/neoip-2.1.0.tgz"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.{js,ts}": "eslint --fix"
  },
  "packageManager": "yarn@3.6.0"
}
