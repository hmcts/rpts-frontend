{
  "name": "rpts-frontend",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=20.16.0"
  },
  "scripts": {
    "start": "NODE_ENV=production ./node_modules/.bin/ts-node -r tsconfig-paths/register src/main/server.ts",
    "start:dev": "nodemon --config nodemon.json",
    "lint": "sass-lint -v -q && eslint . --ext .js,.ts",
    "build": "yarn webpack --config webpack.config.js",
    "build:prod": "NODE_ENV=production yarn webpack --mode production --config webpack.config.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:routes": "jest -c jest.routes.config.js",
    "test:a11y": "jest -c jest.a11y.config.js --maxWorkers 15",
    "sonar-scan": "sonar-scanner",
    "test:smoke": "NODE_TLS_REJECT_UNAUTHORIZED=0 jest -c jest.smoke.config.js",
    "test:functional": "npm-run-all -scl test:codecept test:functional:report",
    "test:functional:report": "allure generate functional-output/functional/reports/ -c -o functional-output/functional/allure",
    "test:codecept": "yarn playwright install && NODE_TLS_REJECT_UNAUTHORIZED=0 codeceptjs run --config ./src/test/functional/codecept.conf.js --steps",
    "cichecks": "yarn && yarn build && yarn lint  && yarn test && yarn test:routes &&  yarn test:a11y"
  },
  "dependencies": {
    "@eslint/js": "^9.25.1",
    "@hmcts/info-provider": "^1.2.3",
    "@hmcts/nodejs-healthcheck": "^1.8.5",
    "@hmcts/nodejs-logging": "^4.0.4",
    "@hmcts/properties-volume": "^1.2.0",
    "@types/config": "^3.3.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/es6-promisify": "^6.0.4",
    "@types/express": "^5.0.1",
    "@types/glob": "^8.1.0",
    "@types/jquery": "^3.5.32",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.15.3",
    "@types/nunjucks": "^3.2.6",
    "@types/require-directory": "^2.1.6",
    "@types/serve-favicon": "^2.5.7",
    "applicationinsights": "3.7.0",
    "axios": "1.9.0",
    "brace-expansion": "4.0.1",
    "config": "^3.3.12",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "express-nunjucks": "^3.1.2",
    "glob": "^11.0.2",
    "globals": "^16.0.0",
    "govuk-frontend": "5.10.1",
    "helmet": "^8.1.0",
    "jquery": "^3.7.1",
    "lodash": "^4.17.21",
    "nunjucks": "^3.2.4",
    "require-directory": "^2.1.1",
    "serve-favicon": "^2.5.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/eslint-parser": "^7.27.1",
    "@babel/preset-env": "^7.27.1",
    "@codeceptjs/allure-legacy": "^1.0.2",
    "@types/chai-as-promised": "^8.0.2",
    "@types/jest": "^29.5.14",
    "@types/jest-when": "^3.5.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "allure-commandline": "2.34.0",
    "allure-playwright": "3.2.1",
    "axios-debug-log": "^1.0.0",
    "babel-loader": "^10.0.0",
    "chai": "5.2.0",
    "chai-as-promised": "^8.0.1",
    "codeceptjs": "^3.7.3",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "debug": "^4.4.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.6",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-html-reporter": "^4.1.0",
    "lint-staged": "^15.5.1",
    "mini-css-extract-plugin": "^2.9.2",
    "nock": "^14.0.4",
    "node-sass": "^9.0.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "4.1.5",
    "pa11y": "^8.0.0",
    "playwright": "^1.52.0",
    "prettier": "^3.5.3",
    "request": "^2.88.2",
    "sass-lint": "^1.13.1",
    "sass-loader": "^16.0.5",
    "sinon": "^20.0.0",
    "sinon-chai": "^4.0.0",
    "sonar-scanner": "^3.1.0",
    "style-loader": "^4.0.0",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "webpack-dev-middleware": "^7.4.2",
    "webpack-node-externals": "^3.0.0"
  },
  "resolutions": {
    "formidable": "3.5.4",
    "cross-spawn": "7.0.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.{js,ts}": "eslint --fix"
  },
  "packageManager": "yarn@4.9.1"
}
