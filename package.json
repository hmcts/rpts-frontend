    "webpack-dev-middleware": "^7.4.2",
    "webpack-node-externals": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "resolutions": {
    "axios": ">=1.7.7",
    "@azure/identity": ">=4.2.1",
    "tar": ">=6.2.1",
    "braces": ">=3.0.3",
    "moment": ">=2.29.4",
    "async": ">=2.6.4",
    "express": ">=4.21.1",
    "follow-redirects": ">=1.15.6",
    "sass-lint/merge": ">=2.1.1",
